{
  "name": "lundalogik/laravel-newsletter-driver",
  "description": "Lime Newsletter mail driver for Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Lundalogik"
    }
  ],
  "autoload": {
    "psr-4": {
      "Lundalogik\\NewsletterDriver\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lundalogik\\NewsletterDriver\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Lundalogik\\NewsletterDriver\\NewsletterMailServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^6.0"
  },
  "require": {
    "guzzlehttp/guzzle": "^7.0"
  }
}
