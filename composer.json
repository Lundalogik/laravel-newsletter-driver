{
    "name": "lundalogik/laravel-newsletter-driver",
    "description": "Laravel Lime Newsletter Mail Driver",
    "type": "library",
    "license": "MIT",
    "version": "2.0.3",
    "authors": [
        {
            "name": "Lundalogik"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lundalogik\\NewsletterDriver\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lundalogik\\NewsletterDriver\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lundalogik\\NewsletterDriver\\NewsletterMailServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^10.0",
        "spatie/temporary-directory": "^2.2"
    },
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/contracts": "^10.0",
        "symfony/mailer": "^6.2"
    }
}